<div class="wrapper">
  <h1>EDIT YOUR PROFILE </h1>

  <%= simple_form_for @user, url: profile_update_path, html: {method: :get} do |f| %>
  <%= f.input :username %>
  <%= f.input :description %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit %>
  <% end %>



</div>
