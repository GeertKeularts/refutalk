<div class="wrapper">
  <h1>REFUTALK</h1>
  <p>Here is a list of all Dutchies waiting to share their Dutch with you!</p>

  <%#= @dutchies %>

  <% @dutchies.each do |dutchie| %>
  <img src="<%= dutchie.photo %>" alt="<%=dutchie.username%>">  <br>
  <%= dutchie.username %> <br>
  <%= dutchie.description %> <br>
  <%= link_to "Go to this profile", dutchy_path(dutchie)  %> <br>
  <hr>
  <% end %>
</div>

